FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    cylinderToCell
    {
        p1 (0.5 0.5 0);
	p2 (0.5 0.5 1);
	radius 0.2;
	fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


